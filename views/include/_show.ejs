<% for(let i=0;i<users.length;i++){ %>
<tr>
   
    <td><%=users[i].name%></td>
    <td><%=users[i].email%></td>
    <td><%=users[i].position%></td>
    <td>
        <a href="/update-user?id=<%=i+1%>" class="btn btn-outline-primary"><i
                class="fa-solid fa-pencil "></i></a>
        <a  class="btn btn-outline-primary delete-btn" data-id="<%=i+1%>"><i
                class="fa-sharp fa-solid fa-trash"></i></a>
    </td>
</tr>
<% } %>
